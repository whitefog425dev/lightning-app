{
  "name": "lightning-desktop",
  "version": "1.0.1",
  "description": "main.js",
  "main": "index.js",
  "engines": {
    "node": "^6.7.0"
  },
  "scripts": {
    "start": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js --icon './assets/ln-logo'",
    "package-all": "npm run package -- --all",
    "dev": "cross-env NODE_ENV=development node -r babel-register dev-server.js",
    "postinstall-old": "cd node_modules/grpc && npm run electron-build -- --target=1.4.6"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": "case <case@casesandberg.com>",
  "license": "MIT",
  "dependencies": {
    "lightning-components": "^0.1.1",
    "lightning-core": "^1.9.1",
    "lightning-notifications": "^0.1.1",
    "lightning-store": "^0.1.1",
    "lodash": "^4.17.2",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^4.0.0-alpha.5",
    "reactcss": "^1.1.1",
    "redux": "^3.6.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "electron-debug": "^1.1.0",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.6",
    "electron-window-state": "^4.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "observe": "^1.3.7",
    "ps-node": "^0.1.4",
    "react": "^15.4.1",
    "redux": "^3.6.0",
    "source-map-support": "^0.4.8",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^1.1.2",
    "webpack-validator": "^2.3.0"
  }
